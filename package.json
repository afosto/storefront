{
  "name": "@afosto/storefront",
  "version": "3.2.0-beta.2",
  "description": "Afosto storefront client",
  "keywords": [
    "afosto",
    "storefront",
    "storefront-client"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/afosto/storefront"
  },
  "author": "Afosto Saas BV",
  "license": "MIT",
  "private": false,
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/afosto/storefront/issues"
  },
  "homepage": "https://github.com/afosto/storefront",
  "scripts": {
    "cleanup": "rimraf dist",
    "dev": "rimraf dist && tsc --watch",
    "format": "prettier --write .",
    "prebuild": "rimraf dist",
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm cleanup && tsup **/*.ts --format esm,cjs --dts"
  },
  "dependencies": {
    "@afosto/graphql-client": "^3.0.0-alpha.7",
    "@types/js-cookie": "^3.0.6",
    "@types/uuid": "^9.0.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
